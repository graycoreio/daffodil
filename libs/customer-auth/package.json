{
  "name": "@daffodil/customer-auth",
  "nx": {
    "targets": {
      "build": {
        "inputs": ["production", "^production"],
        "outputs": ["{workspaceRoot}/dist/customer-auth"]
      }
    }
  },
  "version": "0.0.0-PLACEHOLDER",
  "description": "Interfaces built for daffodil/state",
  "repository": {
    "type": "git",
    "url": "https://github.com/graycoreio/daffodil"
  },
  "author": "Graycore LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graycoreio/daffodil/issues"
  },
  "scripts": {
    "build": "ng build customer-auth --prod",
    "lint": "cd ../.. && ng lint customer-auth",
    "lint:fix": "npm run lint -- --fix",
    "test": "ng test customer-auth --watch=false --browsers=ChromeHeadless",
    "prepublish": "cd ../../dist/customer-auth",
    "publish": "npm publish --access=public"
  },
  "homepage": "https://github.com/graycoreio/daffodil",
  "peerDependencies": {
    "@angular/common": "0.0.0-PLACEHOLDER",
