    "@ngrx/store": "0.0.0-PLACEHOLDER",
    "rxjs": "0.0.0-PLACEHOLDER"
  },
  "devDependencies": {
    "@daffodil/auth": "0.0.0-PLACEHOLDER",
    "@daffodil/customer": "0.0.0-PLACEHOLDER"
  }
