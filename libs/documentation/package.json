{
  "name": "@daffodil/documentation",
  "private": true,
  "nx": {
    "targets": {
      "build": {
        "outputs": ["{workspaceRoot}/dist/documentation"]
      }
    }
  },
  "version": "0.0.0-PLACEHOLDER",
  "author": "Graycore LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/graycoreio/daffodil/issues"
  },
