    "url": "https://github.com/graycoreio/daffodil/issues"
  },
  "scripts": {
    "build": "ng build external-router",
    "lint": "ng lint external-router",
    "test": "ng test external-router --watch=false --browsers=ChromeHeadless",
    "reportcoverage":
      "echo '(../../cc-test-reporter format-coverage -t lcov -o ../../coverage/cc.external-router.json ../../coverage/libs/external-router/lcov.info)'",
    "publish": "(cd ../../dist/external-router; npm publish --access=public)"
