{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc/apps/design-land",
    "paths": {
      "@daffodil/*": [
        "dist/*"
      ],
      "@daffodil/design": [
        "libs/design/src"
      ],
      "@daffodil/design/*": [
        "libs/design/*/src"
      ],
    }
  },
  "exclude": [
    "test.ts",
    "server/*.ts",
    "**/*.spec.ts"
  ],
  "include": [
    "**/*.ts"
  ]
}
