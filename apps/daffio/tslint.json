{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "daffio",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "daffio",
      "kebab-case"
    ],
    "component-class-suffix": [
      true,
      "Component",
      "Container"
    ]
  }
}
