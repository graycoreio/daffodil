{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "daffodil",
    "npmScope": "daffodil",
    "latestMigration": "20180412-nx-update-scripts"
  },
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "./tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/foundation-app/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/foundation-app/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "apps": [
    {
      "name": "foundation-demo",
      "root": "apps/foundation-app/src",
      "outDir": "dist/apps/foundation-demo",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.packaged.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "scss/styles.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "tags": []
    },
    {	
      "name": "foundation-dev",	
      "root": "apps/foundation-app/src",	
      "outDir": "dist/apps/foundation-app",	
      "assets": [	
        "assets",	
        "favicon.ico"	
      ],	
      "index": "index.html",	
      "main": "main.ts",	
      "polyfills": "polyfills.ts",	
      "test": "../../../test.js",	
      "tsconfig": "tsconfig.app.json",	
      "testTsconfig": "../../../tsconfig.spec.json",	
      "prefix": "app",	
      "styles": [	
        "scss/styles.scss"	
      ],	
      "scripts": [],	
      "environmentSource": "environments/environment.ts",	
      "environments": {	
        "dev": "environments/environment.ts",	
        "prod": "environments/environment.prod.ts"	
      },	
      "tags": []	
    },
    {
      "name": "core",
      "root": "libs/core/src",
      "test": "../../../test.js",
      "appRoot": "",
      "tags": []
    },
    {
      "name": "$workspaceRoot",
      "root": ".",
      "appRoot": ""
    }
  ],
  "defaults": {
    "schematics": {
      "collection": "@nrwl/schematics"
    },
    "styleExt": "css",
    "component": {}
  },
  "warnings": {
    "typescriptMismatch": false
  }
}
