{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/jasmine",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "inlineSources": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "types": ["node","jasmine"]
  },
  "exclude": [
    "src/test.ts",
    "**/*.spec.ts"
  ]
}
