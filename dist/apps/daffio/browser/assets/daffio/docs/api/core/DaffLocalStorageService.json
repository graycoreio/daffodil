{
  "id": "api/core/DaffLocalStorageService",
  "title": "DaffLocalStorageService",
  "contents": "\n<header class=\"api-header\">\n  <h1 class=\"api-header__name\">DaffLocalStorageService</h1>\n  <label class=\"api-header__label class\">class</label>\n</header>\n\n\n<p class=\"api-subtitle\"></p>\n\n<div class=\"api-body\">\n  \n\n\n<section class=\"members\">\n  <h2>Properties</h2>\n  <div class=\"members-table\">\n    <div class=\"members-table__header\">Name</div>\n    <div class=\"members-table__header\">Type</div>\n    <div class=\"members-table__header\">Description</div>\n\t\t\t\n<div class=\"members-table__cell\">setItem</div>\n<div class=\"members-table__cell\">void</div>\n<div class=\"members-table__cell\">Persist the given item into local storage.</div>\n\n\n    \n\t\t\t\n<div class=\"members-table__cell\">getItem</div>\n<div class=\"members-table__cell\">any</div>\n<div class=\"members-table__cell\">Retrieve the given item from localstorage.</div>\n\n\n    \n\t\t\t\n<div class=\"members-table__cell\">removeItem</div>\n<div class=\"members-table__cell\">any</div>\n<div class=\"members-table__cell\">Remove a given item from localstorage</div>\n\n\n    \n\t\t\t\n<div class=\"members-table__cell\">clear</div>\n<div class=\"members-table__cell\"></div>\n<div class=\"members-table__cell\"></div>\n\n\n    </div>\n</section>\n\n</div>"
}